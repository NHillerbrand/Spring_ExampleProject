<suppressions
        xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.2.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.2.xsd ">
    <!-- Spring HATEOAS -->
    <suppress>
        <notes><![CDATA[file name: spring-hateoas-1.0.0.M1.jar]]></notes>
        <gav regex="true">^org\.springframework\.hateoas:spring-hateoas:.*$</gav>
        <cpe>cpe:/a:springsource:spring_framework</cpe>
    </suppress>

    <!-- spring-retry -->
    <suppress>
        <notes>file name: spring-retry-1.2.4.RELEASE.jar</notes>
        <gav regex="true">^org\.springframework\.retry:spring-retry:.*$</gav>
        <cpe>cpe:/a:springsource:spring_framework</cpe>
    </suppress>

    <!-- Eureka Client -->
    <suppress>
        <notes><![CDATA[file name: netflix-commons-util-0.3.0.jar]]></notes>
        <gav regex="true">^com\.netflix\.netflix-commons:netflix-commons-util:.*$</gav>
        <cve>CVE-2017-2616</cve>
    </suppress>
    <suppress>
        <notes><![CDATA[file name: guava-19.0.jar]]></notes>
        <gav regex="true">^com\.google\.guava:guava:.*$</gav>
        <cve>CVE-2018-10237</cve>
    </suppress>

    <!-- Spring Integration -->
    <suppress>
